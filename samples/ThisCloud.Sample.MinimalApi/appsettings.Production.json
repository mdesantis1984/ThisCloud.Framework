{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  
  "ThisCloud": {
    "Web": {
      "ServiceName": "sample-minimal-api",
      
      "Cors": {
        "Enabled": true,
        "AllowedOrigins": [
          "https://production.thiscloud.com"
        ],
        "AllowCredentials": true
      },
      
      "Swagger": {
        "Enabled": false,
        "RequireAdmin": true,
        "AllowedEnvironments": []
      },
      
      "Cookies": {
        "SecurePolicy": "Always",
        "SameSite": "Strict",
        "HttpOnly": true
      },
      
      "Compression": {
        "Enabled": false
      }
    }
  }
}
