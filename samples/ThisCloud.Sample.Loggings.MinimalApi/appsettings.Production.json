{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ThisCloud": {
    "Loggings": {
      "MinimumLevel": "Information",
      "Console": {
        "Enabled": false
      },
      "File": {
        "Enabled": true,
        "Path": "/var/log/thiscloud-sample/log-.ndjson",
        "RollingFileSizeMb": 10,
        "RetainedFileCountLimit": 90
      },
      "Admin": {
        "Enabled": false
      }
    }
  }
}
