{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "ThisCloud": {
    "Loggings": {
      "MinimumLevel": "Debug",
      "Console": {
        "Enabled": true
      },
      "File": {
        "Enabled": true,
        "Path": "logs/log-.ndjson"
      },
      "Admin": {
        "Enabled": true,
        "AllowedEnvironments": [ "Development" ],
        "RequireAdmin": true
      }
    }
  }
}

